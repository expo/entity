  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/lru-cache": "^5.1.1",
    "@types/node": "^20.14.1",
    "ctix": "^2.7.0",
    "eslint": "^8.57.1",
    "eslint-config-universe": "^14.0.0",
    "eslint-plugin-tsdoc": "^0.3.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "ts-jest": "^29.2.5",
    "ts-mockito": "^2.6.1",
    "typescript": "^5.7.3"
  }
