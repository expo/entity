{
  "options": [
    {
      "mode": "bundle",
      "project": "tsconfig.json",
      "exportFilename": "index.ts",
      "useSemicolon": true,
      "useBanner": false,
      "useTimestamp": false,
      "quote": "'",
      "directive": "",
      "fileExt": "none",
      "overwrite": true,
      "backup": false,
      "generationStyle": "default-alias-named-star",
      "include": [
        "src/**"
      ],
      "exclude": [
        "**/*tests__/**",
        "**/__testfixtures__/**"
      ],
      "output": "src"
    }
  ]
}