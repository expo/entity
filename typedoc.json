{
  "entryPoints": ["packages/*"],
  "entryPointStrategy": "packages",
  "includeVersion": false,
  "packageOptions": {
    "includeVersion": false,
    "excludeInternal": true,
    "entryPoints": ["src/index.ts"],
  },
  "out": "doc",
  "exclude": [
    "**/__tests__/**/*.ts",
    "**/__integration-tests__/**/*.ts",
    "**/__testfixtures__/**/*.ts",
    "packages/entity-example",
    "packages/entity-full-integration-tests",
  ],
  "name": "Entity",
  "readme": "README.md"
}